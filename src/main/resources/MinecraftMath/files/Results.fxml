<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="576.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MinecraftMath.files.ResultsController">
    <ImageView fitHeight="576.0" fitWidth="1024.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@../assets/Dirt-Background.png" />
    </ImageView>
    <ImageView fitWidth="700.0" layoutX="161.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@../assets/Logo.png" />
    </ImageView>

    <AnchorPane layoutX="141.0" layoutY="88.0" prefHeight="400.0" prefWidth="720.0">
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#ece9d8" height="400.0" stroke="TRANSPARENT" strokeType="INSIDE" strokeWidth="0.0" width="740.0" />
        <Label fx:id="resultsText" layoutX="251.0" layoutY="80.0" prefHeight="58.0" prefWidth="135.0" style="-fx-font-family: Minecraft; -fx-font-size: 30;" text="Results">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Button fx:id="landingButton" layoutX="420.0" layoutY="295.0" mnemonicParsing="false" onAction="#goLanding" style="-fx-font-family: Minecraft; -fx-font-size: 12;" text="Go to Landing Page" />
      <Button fx:id="openSnake" layoutX="104.0" layoutY="295.0" mnemonicParsing="false" onAction="#openSnake" text="LAUNCH MINIGAME" visible="false" />
      <ImageView fx:id="eggImage" fitHeight="150.0" fitWidth="200.0" layoutX="30.0" layoutY="158.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@../assets/Dragon_Egg.png" />
         </image>
      </ImageView>
    </AnchorPane>

    <ImageView fx:id="avatarLoc" fitHeight="217.0" fitWidth="120.0" layoutX="350.0" layoutY="150.0" pickOnBounds="true" preserveRatio="true" x="550.0" y="200.0" />
    <Label fx:id="nameLabel" alignment="CENTER" layoutX="902.0" layoutY="355.0" prefHeight="25.0" prefWidth="116.0" style="-fx-font-family: Minecraft; -fx-font-size: 20;" textFill="WHITE" />

    <Button layoutY="466.0" mnemonicParsing="false" onAction="#exitAction" prefHeight="100.0" prefWidth="100.0" style="-fx-background-color: transparent;">
        <graphic>
            <ImageView fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@../assets/exit-icon.png" />
            </ImageView>
        </graphic>
    </Button>
</AnchorPane>
